    "tokenColors": [
        {
            "settings": {
                "foreground": "#abb2bf",
            }
        },
        {
            "name": "White",
            "scope": [
                "meta.embedded.line",
                "meta.function-call.arguments",
                "punctuation.accessor",
                "punctuation.separator",
                "punctuation.separator.annotation",
                "punctuation.definition.arguments",
                "support.type.exception",
                "variable.other",
                "variable.other.object.property",
            ],
            "settings": {
                "foreground": "#abb2bf",
            }
        },
        {
            "name": "Italic",
            "scope": [
                "support.class",
                "variable.parameter.function.language",
                "variable.parameter.function-call",
            ],
            "settings": {
                "fontStyle": "italic",
            }
        },
        //-----------------------------
        // Strings
        //-----------------------------
        {
            "name": "Strings",
            "scope": [
                "constant.other.character-class",
                "string",
                "string.regexp.quoted",
            ],
            "settings": {
                "foreground": "#98c379",
            }
        },
        {
            "name": "Escape Characters, Embedded Constants & Punctuation",
            "scope": [
                "constant.character.escape.regexp",
                "constant.escape",
                "punctuation.section.embedded",
                "storage.type.string",
                "string.regexp",
                "support.other.match",
            ],
            "settings": {
                "foreground": "#c678dd",
            }
        },
        {
            "name": "Comments & Docstrings",
            "scope": [
                "comment",
                "string.quoted.docstring",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#626772d3",
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*",
            ],
            "settings": {
                "fontStyle": "italic underline",
            }
        },
        //-----------------------------
        // Syntax
        //-----------------------------
        {
            "name": "Booleans, Symbols, Operators",
            "scope": [
                "constant.language",
                "constant.other.symbol",
                "keyword.operator",
                "keyword.other",
                "constant.character.format",
                "punctuation.separator.statement",
                "punctuation.terminator.statement",
                "punctuation.definition.logical-expression",
            ],
            "settings": {
                "foreground": "#c678dd",
            }
        },
        {
            "name": "Numbers, Punctuation Separators, Library Constants",
            "scope": [
                "constant.numeric",
                "punctuation.separator.object",
                "punctuation.separator.annotation",
            ],
            "settings": {
                "foreground": "#d19a66",
            }
        },
        {
            "name": "Constants, Arguments/Parameters, Class Variables/Fields",
            "scope": [
                "constant.other.caps",
                "string.constant.other.placeholder",
                "variable.parameter",
                "variable.parameter.function.language.special.self",
                "punctuation.definition.variable",
                "variable.other.block",
                "variable.other.readwrite.class",
                "variable.other.readwrite.global",
                "variable.other.readwrite.instance",
                "variable.other.normal.shell"
            ],
            "settings": {
                "foreground": "#e06c75",
            }
        },
        {
            "name": "Decorator Indicators",
            "scope": [
                "punctuation.definition.decorator",
            ],
            "settings": {
                "foreground": "#e06c75",
                "fontStyle": "italic",
            }
        },
        {
            "name": "Imports, Requires, Includes, Control, end, Operator, Misc",
            "scope": [
                "keyword.control",
                "keyword.control.import",
                "keyword.control.pseudo-method",
                "keyword.control.ruby",
                "keyword.operator.other.ruby",
                "keyword.other.special-method",
                "keyword.other.substitution",
                "keyword.other.template",
                "constant.other.color",
            ],
            "settings": {
                "foreground": "#d19a66"
            }
        },
        {
            "name": "Class Names & Namespaces",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "entity.name.type.class",
            ],
            "settings": {
                "foreground": "#61afef",
            }
        },
        {
            "name": "Support Constants & Variables",
            "scope": [
                "support.constant",
                "support.class.ruby",
                "support.other.namespace",
                "support.other.variable",
            ],
            "settings": {
                "foreground": "#61afef",
            }
        },
        {
            "name": "Inherited Class Names",
            "scope": [
                "entity.other.inherited-class",
            ],
            "settings": {
                "foreground": "#e5c07b",
                "fontStyle": "italic",
            }
        },
        {
            "name": "Keywords for Classes, Functions, Modules, Storage",
            "scope": [
                "keyword.control.class",
                "keyword.control.def",
                "keyword.control.module",
                "storage.modifier",
                "storage.type",
            ],
            "settings": {
                "foreground": "#c678dd",
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.class.ruby",
                "support.function",
                "variable.other.assignment.shell",
                "variable.function",
                // imports in Ruby are perceived as constants
                "variable.other.constant.ruby",
            ],
            "settings": {
                "foreground": "#61afef",
            }
        },
        {
            "name": "Decorators, Support Types",
            "scope": [
                "support.type",
                "support.type.python",
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#abb2bf",
            }
        },
        {
            "name": "Language Specials",
            "scope": [
                "variable.language.self",
                "variable.language.special",
                "variable.parameter.function.language.special",
                "variable.language.this",
            ],
            "settings": {
                "foreground": "#d27b53",
                "fontStyle": "italic",
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
            ],
            "settings": {
                "foreground": "#f76d6f",
            }
        },
        {
            "name": "Invalid Deprecated",
            "scope": [
                "invalid.deprecated",
            ],
            "settings": {
                "foreground": "#523d14",
            }
        },
        {
            "name": "Tags",
            "scope": [
                "meta.tag",
            ],
            "settings": {
                "foreground": "#abb2bf",
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
            ],
            "settings": {
                "foreground": "#e5c07b",
            }
        },
        {
            "name": "Tag Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e06c75"
            }
        },
        //-----------------------------
        // SQL
        //-----------------------------
        {
            "name": "SQL Keywords",
            "scope": [
                "keyword.other.sql",
                "keyword.other.DML.sql",
            ],
            "settings": {
                "foreground": "#d19a66",
            }
        },
        {
            "name": "SQL Functions, Storage Types",
            "scope": [
                "storage.type.sql",
            ],
            "settings": {
                "foreground": "#61afef",
            }
        },
        {
            "name": "SQL Numbers",
            "scope": [
                "constant.numeric.sql",
            ],
            "settings": {
                "foreground": "#d3735b",
            }
        },
        //-----------------------------
        // Markup
        //-----------------------------
        {
            "name": "Markup Heading",
            "scope": [
                "markup.heading",
            ],
            "settings": {
                "foreground": "#d19a66",
            }
        },
        {
            "name": "Markup Heading Definition",
            "scope": [
                "punctuation.definition.heading",
            ],
            "settings": {
                "foreground": "#e06c75",
            }
        },
        {
            "name": "Markup links",
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#61afef",
            }
        },
        {
            "name": "Deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e06c75",
            }
        },
        {
            "name": "Inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#c3e88d",
            }
        },
        {
            "name": "Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#61afef",
            }
        },
